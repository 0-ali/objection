    "url": "https://github.com/sensepost/objection/issues"
  },
  "homepage": "https://github.com/sensepost/objection#readme",
  "dependencies": {
    "frida-java": "^1",
    "frida-objc": "^2",
    "frida-screenshot": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7.1",
    "@babel/plugin-transform-runtime": "^7.1",
    "@babel/preset-env": "^7.1",
    "@babel/runtime-corejs2": "^7.1",
    "@types/node": "^10.11",
    "frida-compile": "^8",
    "frida-gum-types": "^2.0.4",
    "tslint": "^5"
